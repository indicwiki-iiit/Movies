	{%- endif %}
	{%- if stars != 'NaN' %}
	{%- set line =[
		"ఈ చిత్రం లో నటించిన ప్రముఖ నటులు "+stars+".",
		stars +"ఈ చిత్ర ప్రముఖ నటులు."]|random
		-%}
		{{-line-}}
	{%- endif %}
	{%- if composer != 'NaN' %}
