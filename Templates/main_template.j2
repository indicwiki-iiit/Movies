{%- macro introp1(title, year, genre, director,writer,producer,production_company,stars,composer,film_locations) -%}
	==పరిచయం==
	{{title}}  అనే చిత్రం {{year}} లో విడుదల అయినది.
	ఈ చిత్ర శైలి {{genre}}.
	{%- if writer != 'NaN' %}
	ఈ చిత్రానికి {{director}} దర్శకత్వం వహించగా , {{writer}} రచించారు.
	{%- endif %}
	{%- if producer != 'NaN' %}
	{{title }}  నిర్మాత  {{producer}}, నిర్మాణ సంస్థ {{production_company}}.
	{%- endif %}
	{%- if stars != 'NaN' %}
	ఈ చిత్రం లో నటించిన ప్రముఖ నటులు {{stars}}.
	{%- endif %}
	{%- if composer != 'NaN' %}
	ఈ చిత్రానికి సంగీత దర్శకత్వం {{composer}} అందించారు.
	{%- endif %}
	{%- if film_locations != 'NaN' %}
	ఈ చిత్రము {{film_locations}}ప్రాంతములో చిత్రీకరించబడినది.
	{%- endif %}
	{{'\n'}}
{%- endmacro -%}


{%- macro introp2(budget, year, languages, countries ,storyline,based_on) %}
	{%- if budget != 'NaN' -%}
	 ఈ చిత్రాన్ని తయారు చేయడానికి {{budget}} ఖర్చు పెట్టారు.
	{%- endif -%}
	{{year}} లో విడుదల అయిన ఈ చిత్రం {{languages}} భాషలో, {{countries}}    దేశం/ దేశాల్లో విడుదల చేయబడింది.
	{%- if storyline != 'NaN' %}
	ఈ చిత్ర కథాంశం  {{storyline}}
	{%- endif %}
	{%- if based_on != 'NaN' %}
	ఈ చిత్రం {{based_on}} ఆధారంగా చిత్రీకరించబడింది.
	{%- endif %}
{%- endmacro -%}

{%- macro crew(cast,Director, writers, producer, composer, film_editor, cinematography, casting, production_design, set_decoration, art_design) -%}
    {%- if cast != "NaN" or  Director != "NaN" or writers!= "NaN" or producer!= "NaN" or film_editor!= "NaN" or cinematography!= "NaN" or casting!= "NaN" or production_design!= "NaN" or set_decoration!= "NaN" or art_design!= "NaN" -%}	
		{{ '\n==తారాగణం==\n' }}
		{%- if cast != "NaN" -%}
			{{ '\n===నటీ నటులు, పాత్రలు===\n' }}
			{%- for c in cast -%}
			* {{ c + '\n' }}
			{%- endfor -%}
		{%- endif %}

		{%- if Director != "NaN" or writers!= "NaN" or producer!= "NaN" or composer!= "NaN" or film_editor!= "NaN" or cinematography!= "NaN" or casting!= "NaN" or production_design!= "NaN" or set_decoration!= "NaN" or art_design!= "NaN" -%}	
			{{ '\n===సాంకేతిక సిబ్బంది===\n' }}
			{%- if Director != "NaN" -%}
				ఈ చిత్ర ధర్శకుడు {{Director}}. 
			{%- endif %}

			{%- if writers != "NaN" -%}
				ఈ చిత్ర రచన {{writers}} చేపట్టారు. 
			{%- endif %}

			{%- if producer != "NaN" -%}
				ఈ చిత్ర నిర్మాతగా {{producer}} వ్యవహరించారు.
			{%- endif %}

			{%- if composer != "NaN" -%}
				ఈ చిత్రానికి సంగీత దర్శకత్వం {{composer}} అందించారు.
			{%- endif %}

			{%- if film_editor != "NaN" -%}
				ఈ చిత్రానికి సంపాదకులు గా {{film_editor}} చేశారు. 
			{%- endif %}

			{%- if cinematography != "NaN" -%}
				చిత్రీకరణ {{cinematography}} చేశారు. 
			{%- endif %}

			{%- if casting != "NaN" -%}
				ఈ చిత్రానికి తారాగణాన్ని  ఎంచుకున్న వ్యక్తి {{casting}}. 
			{%- endif %}

			{%- if production_design != "NaN" -%}
				{{production_design}} ఈ చిత్ర నిర్మాణాన్ని రూపు కల్పించారు. 
			{%- endif %}

			{%- if set_decoration != "NaN" -%}
				ఈ చిత్రానికి కళా దర్శకత్వం చేసినది {{set_decoration}}. 
			{%- endif %}

			{%- if art_design != "NaN" -%}
				ఈ చిత్రానికి కళ రూపకల్పన చేసినది {{art_design}}. 
			{%- endif %}

		{%- endif %}
	{%- endif %}
{%- endmacro -%}

{%- macro songs_table(Songs,lensong,songcomma,composer,sound_mix,IMDbID) -%}
    {%- if Songs != "NaN" or  composer != "NaN" or sound_mix!= "NaN" -%}	
		{{ '\n==సౌండ్ ట్రాక్==\n' }}

		{%- if composer != "NaN" -%}
		ఈ చిత్రానికి సంగీత దర్శకత్వం {{ composer }} అందించారు. 
		{%- endif %}
		
		{%- if sound_mix != "NaN" -%}
		ఈ సినిమా సౌండ్ మిక్స్ {{ sound_mix }}. 
		{%- endif %}

		{%- if Songs != "NaN" -%}
			ఈ చిత్రం లో మొత్తం {{ lensong }} పాటలు ఉన్నాయి. ఈ చిత్ర పాటల జాబితా క్రింద ఇవ్వబడింది.<ref name="song">[https://www.imdb.com/title/{{IMDbID}}/soundtrack?ref_=tt_trv_snd/ {{Title}} Songs]</ref>	
			{{ '\n' }}
			{%- if songcomma == "True"  -%}
				{| class="wikitable"
				|-
				! పాట !! గాయకుడు / గాయని  !! సమయం 
				{% for song in Songs -%}
					|-
					| {{ song[0] }} || {{ song[1] }} || {{ song[2] }}
					{{ '\n' }}
				{%- endfor -%}
				|}
			{%- elif songcomma == "False" -%}
				{| class="wikitable"
				|-
				! పాటలు
				{% for song in Songs -%}
					|-
					| {{ song }}
					{{ '\n' }}
				{%- endfor -%}
				|}
			{%- endif %}
		{%- endif %}
	{%- endif %}

	References: {{reflist}}
{%- endmacro -%}

{%- macro winner(Winner,Nominee) -%}
    {{ '\n==పురస్కారాలు==\n' }}
    {% if Winner != 'NaN' and Nominee != 'NaN'%}
    ఈ చిత్రానికి పలు పురస్కారాలు, నామినేషన్స్ వివిధ క్యాటగిరీస్ లో లభించాయి. వాటిలో కొన్ని వివరాలు క్రింద ఇవ్వబడ్డాయి.
    {%- endif -%}
    {% if Winner != 'NaN' and Nominee == 'NaN' %}}
    ఈ చిత్రానికి పలు పురస్కారాలు వివిధ క్యాటగిరీస్ లో లభించాయి. వాటిలో కొన్ని వివరాలు క్రింద ఇవ్వబడ్డాయి.
    {%- endif -%}
    {% if Nominee != 'NaN' and Winner == 'NaN' %}}
    ఈ చిత్రానికి పలు నామినేషన్స్ వివిధ క్యాటగిరీస్ లో లభించాయి. వాటిలో కొన్ని వివరాలు క్రింద ఇవ్వబడ్డాయి.
    {%- endif -%}
    {% if Winner != "NaN" %}
        {% for item in Winner %}
            {% for values in Winner[item]%}
                |-
                |{{item}}
                |{{values[0]}}
                |{{values[1]}}
                |{{win}}
            {%- endfor -%}
        {%- endfor -%}
    {%- endif -%}
    -------------------
    {% if Nominee != "NaN" %}
        {% for item in Nominee %}
            {% for values in Nominee[item]%}
                |-
                |{{item}}
                |{{values[0]}}
                |{{values[1]}}
                |{{nom}}
            {%- endfor -%}
        {%- endfor -%}
    {%- endif -%}
{%- endmacro -%}

{%- macro Technical_Details(Duration,sound_mix,colors) -%}
{%- if sound_mix!="NaN" or Duration!="NaN" or colors!="NaN"-%}
{#{ '\n' ==సాంకేతిక వివరాలు== '\n' }#}
<h3>సాంకేతిక వివరాలు</h3>
{%- if Duration!=0-%}
    ఈ చిత్ర పూర్తి వ్యవధి {{Duration}} నిమిషాలు .
{%-endif-%}
{%-if sound_mix!="NaN"-%}
	ఈ చిత్రానికి సౌండ్ టెక్నాలజీ {{sound_mix}} అందించింది.
{%-endif-%}
{%-if colors!="NaN"-%}
  ఈ చిత్రం చిత్రికరణ {{colors}} లో తీయబడింది.
