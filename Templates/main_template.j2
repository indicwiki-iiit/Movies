{%- macro critics_response(Votes,Rating) -%}
{%- if Votes!="NaN" or Rating!="NaN"  -%}
<h3>ప్రతిస్పందన</h3>
{#{ ==ప్రతిస్పందన== }#}
		{%- if Votes!="NaN" and Rating!="NaN" -%}
			మొత్తం {{Votes}} మంది వేసిన ఓట్ల ఆధారంగా ఈ చిత్రానికి IMDb లో {{Rating}} రేటింగ్ లభించింది.
		{%- elif Votes!="NaN" or Rating=="NaN" -%}
			ఈ చిత్రానికి మొత్తం{{Votes}} వచ్చాయి.
		{%- elif Votes=="NaN" or Rating!="NaN" -%} 
			ఈ చిత్రానికి ఐ.ఎమ్.డీ.బి.{{Rating}} లో లభించింది 
		{%- endif -%}

{%- endif -%}
{%- endmacro -%}
{%- macro production_boxOffice(production_company,budget,opening_weekend,Gross,cumulative) -%}

{%- if production_company!="NaN" or budget!="NaN" or opening_weekend!="NaN" or Gross!="NaN" or  cumulative!="NaN"  -%}
{#{ ==నిర్మాణం, బాక్స్ ఆఫీస్== }#}
<h3>నిర్మాణం, బాక్స్ ఆఫీస్</h3>
		{%- if production_company!="NaN" and budget!="NaN" -%}
			నిర్మాణ సంస్థ {{production_company}} ,{{budget }} బడ్జెట్ తో ఈ చిత్రాన్ని నిర్మించారు .
		{%- elif production_company!="NaN" or budget=="NaN" -%}
			{{production_company}} అనే  నిర్మాణ సంస్థ ఈ చిత్రాన్ని నిర్మించారు.
		{%- elif production_company=="NaN" or budget!="NaN" -%}
			ఈ చిత్రానికి నిర్మాణ సంస్థ {{budget }} బడ్జెట్ తో నిర్మించారు.
		{%- endif -%}

		{%- if opening_weekend!="NaN" -%}
			ఈ చిత్రం మొదటి వారంలో {{opening_weekend}} డాలర్లు వసూలు చేసింది.
		{%- endif -%}
		
		{%- if Gross!="NaN"-%}
		 	ఈ చిత్రం మొత్తం అమెరికాలో{{Gross}} డాలర్లు వసూలు చేసింది.
		{%- endif -%}
		
		{%- if cumulative!="NaN"-%}
		 	ప్రపంచవ్యాప్తంగా ఈ చిత్రం {{cumulative}} డాలర్లు వసూలు చేసింది.
		{%- endif -%}
{%- endif -%}
{%- endmacro -%}

